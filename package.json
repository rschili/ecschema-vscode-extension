{
	"name": "ecschema-vscode-extension",
	"displayName": "ecschema vscode extension",
	"description": "Extension for ECSchema grammar",
	"version": "0.0.1",
	"publisher": "rschili",
	"private": true,
	"license": "MIT",
	"pricing": "Free",
	"repository": {
		"type": "git",
		"url": "https://github.com/rschili/ecschema-vscode-extension"
	},
	"engines": {
		"vscode": "^1.98.2"
	},
	"categories": [
		"Linters",
		"Other"
	],
	"activationEvents": [
		"onLanguage:xml"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@eslint/js": "^9.32.0",
		"@stylistic/eslint-plugin": "^5.2.2",
		"@types/node": "^24",
		"@types/vscode": "^1.102.0",
		"eslint": "^9.32.0",
		"generate-license-file": "^4.0.0",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.38.0"
	},
	"dependencies": {
		"@xml-tools/ast": "^5.0.5",
		"@xml-tools/ast-position": "^2.0.7",
		"@xml-tools/constraints": "^1.1.1",
		"@xml-tools/content-assist": "^3.1.11",
		"@xml-tools/parser": "^1.0.11",
		"@xml-tools/validation": "^1.0.16"
	}
}
